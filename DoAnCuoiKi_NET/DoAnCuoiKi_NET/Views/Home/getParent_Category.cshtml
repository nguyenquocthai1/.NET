@model List<DoAnCuoiKi_NET.Models.Category>
<div class="container">
    @foreach (var i in Model)
    {
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title">
                    <h2><a href="@ViewBag.meta/@i.meta" style="color:lightcoral">@i.name_cate</a></h2>
                </div>
            </div>
        </div>
        {
            {
                @Html.Action("getParent_Product", "Home", new { meta_parent = @i.meta, meta = ViewBag.meta })
            }

        }
    }
</div>

